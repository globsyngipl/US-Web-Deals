@model deals.earlymoments.com.Models.ShippingModels.ShippingAddress

<div class="row marginBot6">
    <div class="col-md-4 col-sm-4"></div>
    <div class="col-md-8 col-sm-8 bold">
        <span>*</span> Required field
    </div>
</div>
<div class="row marginBot6">
    <div class="col-md-4 col-sm-4 col-xs-4 formLabel">
        <label><span>*</span> First Name</label>
    </div>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <!--<input type="text" maxlength="15" class="form-control" id="txtFirstName">-->
        @Html.TextBoxFor(model => model.ShippingFirstName, new { @id = "txtShipFirstName", @maxlength = "50", @class = "form-control" })
    </div>
</div>
<div class="row marginBot6">
    <div class="col-md-4 col-sm-4 col-xs-4 formLabel">
        <label><span>*</span> Last Name</label>
    </div>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <!-- <input type="text" maxlength="15" class="form-control" id="txtLastName"> -->
        @Html.TextBoxFor(model => model.ShippingLastName, new { @id = "txtShipLastName", @maxlength = "50", @class = "form-control" })
    </div>
</div>
<div class="row marginBot6">
    <div class="col-md-4 col-sm-4 col-xs-4 formLabel">
        <label><span>*</span> Address</label>
    </div>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <!-- <input type="text" maxlength="30" class="form-control" id="txtAddress">-->
        @Html.TextBoxFor(model => model.ShippingAddress1, new { @id = "txtShipStreet", @maxlength = "30", @class = "form-control" })
    </div>
</div>
<div class="row marginBot6">
    <div class="col-md-4 col-sm-4 col-xs-4 formLabel">
        <label>Address 2</label>
    </div>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <!--<input type="text" maxlength="30" class="form-control" id="txtAddress2">-->
        @Html.TextBoxFor(model => model.ShippingAddress2, new { @id = "txtShipStreet2", @maxlength = "30", @class = "form-control" })

    </div>
</div>
<div class="row marginBot6">
    <div class="col-md-4 col-sm-4 col-xs-4 formLabel">
        <label><span>*</span> City</label>
    </div>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <!--<input type="text" maxlength="16" class="form-control" id="txtCity">-->
        @Html.TextBoxFor(model => model.ShippingCity, new { @id = "txtShipCity", @maxlength = "50", @class = "form-control" })
